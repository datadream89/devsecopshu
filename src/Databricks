import pandas as pd

# Define the start and end strings for extraction
start_string = "start"
end_string = "end"

# Read the .log file
with open('your_log_file.log', 'r') as file:
    lines = file.readlines()

# List to store extracted values
extracted_values = []

# Flag to indicate whether we're inside the extraction range
inside_extraction = False

# Iterate through each line in the log file
for line in lines:
    if start_string in line:
        inside_extraction = True
        extracted_value = line.split(start_string, 1)[1].strip()
    elif end_string in line and inside_extraction:
        inside_extraction = False
        extracted_value += " " + line.split(end_string, 1)[0].strip()
        extracted_values.append(extracted_value)
    elif inside_extraction:
        extracted_value += " " + line.strip()

# Create a DataFrame with the extracted values
df = pd.DataFrame({'extracted_values': extracted_values})

# Print the DataFrame
print(df)
