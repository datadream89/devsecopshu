import com.typesafe.config.{Config, ConfigFactory}

object ConfigParser {
  def main(args: Array[String]): Unit = {
    val config1: Config = ConfigFactory.parseResources("config1.conf")
    val config2: Config = ConfigFactory.parseResources("config2.conf")

    // Access values from config1
    val value1: String = config1.getString("section1.key1")
    val value2: Int = config1.getInt("section1.key2")

    // Access values from config2
    val value3: Boolean = config1.getBoolean("section2.key3")
    val value4: String = config2.getString("section3.key4")
    val value5: Double = config2.getDouble("section4.key5")

    // Use the values as needed
    println(s"value1: $value1")
    println(s"value2: $value2")
    println(s"value3: $value3")
    println(s"value4: $value4")
    println(s"value5: $value5")
  }
}
